        @* Pagination section: START *@
        <nav aria-label="Page navigation" class="text-right">
            <ul class="pagination">
                @{
                    int currentPage = ViewBag.currentPage ?? 1;
                    int totalPage = ViewBag.totalPage ?? 1;
                    int spacePage = 2;
                    int nearLastPage = totalPage - 1;

                    if (totalPage >= 1)
                    {
                        // Previous button, disable if is first page
                        if (currentPage == 1)
                        {
                            <li class="page-item disabled" aria-disabled="true" aria-label="&lsaquo; Previous">
                                <span class="page-link" aria-hidden="true">&lsaquo;</span>
                            </li>
                        }
                        else
                        {
                            <li class="page-item">
                                <a class="page-link" href='@Url.Action("LienMinh", new {page = currentPage - 1 })'
                            aria-label="&laquo; Previous">&lsaquo;</a>
                            </li>
                        }

                        // Pages
                        // if total of page (less than) < 
                        if (totalPage < 7 + (spacePage * 2))
                        {
                            for (int i = 1; i <= totalPage; i++)
                            {
                                // Disable if is current page
                                if (currentPage == i)
                                {
                                    <li class="page-item active" aria-current="page">
                                        <span class="page-link">@i</span>
                                    </li>
                                }
                                else
                                {
                                    <li class="page-item">
                                        <a class="page-link" href='@Url.Action("LienMinh", new {page = @i })'>@i</a>
                                    </li>
                                }
                            }
                        }
                        else if (totalPage > 7 + (spacePage * 2))
                        {
                            if (currentPage < 1 + (spacePage * 2))
                            {
                                // Beginning only hide later pages
                                for (int i = 1; i <= totalPage - (8 + 2 * spacePage); i++)
                                {
                                    // Disable if is current page
                                    if (currentPage == i)
                                    {
                                        <li class="page-item active" aria-current="page">
                                            <span class="page-link">@i</span>
                                        </li>
                                    }
                                    else
                                    {
                                        <li class="page-item">
                                            <a class="page-link" href='@Url.Action("LienMinh", new {page = @i })'>@i</a>
                                        </li>
                                    }
                                }
                                <li class="page-item disabled" aria-disabled="true"><span class="page-link">...</span></li>
                                <li class="page-item"><a class="page-link" href='@Url.Action("LienMinh", new {page = @totalPage - 1 })'>@nearLastPage</a></li>
                                <li class="page-item"><a class="page-link" href='@Url.Action("LienMinh", new {page = @totalPage })'>@totalPage</a></li>
                            }
                            else if (totalPage - (2 * spacePage) > currentPage && currentPage > (spacePage * 2))
                            {
                                // Middle hide some front and some back
                                <li class="page-item"><a class="page-link"
                            href='@Url.Action("LienMinh", new {page = @totalPage - totalPage + 1 })'>1</a></li>
                                <li class="page-item"><a class="page-link"
                            href='@Url.Action("LienMinh", new {page = @totalPage - totalPage + 2 })'>2</a></li>
                                <li class="page-item disabled" aria-disabled="true"><span class="page-link">...</span></li>

                                for (int i = currentPage - spacePage; i <= currentPage + spacePage; i++)
                                {
                                    // Disable if is current page
                                    if (currentPage == i)
                                    {
                                        <li class="page-item active" aria-current="page">
                                            <span class="page-link">@i</span>
                                        </li>
                                    }
                                    else
                                    {
                                        <li class="page-item">
                                            <a class="page-link" href='@Url.Action("LienMinh", new {page = @i })'>@i</a>
                                        </li>
                                    }
                                }
                                <li class="page-item disabled" aria-disabled="true"><span class="page-link">...</span></li>
                                <li class="page-item"><a class="page-link"
                            href="@Url.Action("LienMinh", new {page = @totalPage - 1 })">@nearLastPage</a></li>
                                <li class="page-item"><a class="page-link"
                            href='@Url.Action("LienMinh", new {page = @totalPage })'>@totalPage</a></li>
                            }
                            else
                            {
                                // End only hide early pages
                                <li class="page-item"><a class="page-link"
                            href='@Url.Action("LienMinh", new {page = @totalPage - totalPage + 1 })'>1</a></li>
                                <li class="page-item"><a class="page-link"
                            href='@Url.Action("LienMinh", new {page = @totalPage - totalPage + 2 })'>2</a></li>
                                <li class="page-item disabled" aria-disabled="true"><span class="page-link">...</span></li>

                                for (int i = totalPage - (1 + 2 * spacePage); i <= totalPage; i++)
                                {
                                    // Disable if is current page
                                    if (currentPage == i)
                                    {
                                        <li class="page-item active" aria-current="page">
                                            <span class="page-link">@i</span>
                                        </li>
                                    }
                                    else
                                    {
                                        <li class="page-item">
                                            <a class="page-link" href='@Url.Action("LienMinh", new {page = @i })'>@i</a>
                                        </li>
                                    }
                                }
                            }
                        }


                        // Next button, disable if is last page
                        if (currentPage == totalPage)
                        {
                            <li class="page-item disabled" aria-disabled="true" aria-label="Next &rsaquo;">
                                <span class="page-link" aria-hidden="false">&rsaquo;</span>
                            </li>
                        }
                        else
                        {
                            <li class="page-item">
                                <a class="page-link" href="@Url.Action("LienMinh", new {page = currentPage + 1 })"
                            aria-label="Next &rsaquo;">&rsaquo;</a>
                            </li>
                        }
                    }
                }
            </ul>
        </nav>
